<h1>Teams</h1>

<p><em>Registration has not begun, so no teams have been created yet.</em></p>

<ol>
  <% @teams.each do |team| %>
    <li>
      <%= team.name %>
      <ul>
	<% team.users.each do |user| %>
	  <li>
	    <%= user.username %>
	  </li>
	<% end %>
      </ul>
    </li>
  <% end %>
</ol>

